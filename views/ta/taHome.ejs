<div class="col-xl-10 offset-md-1">
  <h4 class="my-5">Solve Doubts</h4>
  <div id="doubt-container" class="">
    <% for( let doubt of doubts ) {%>
    <div
      id="doubt-<%=doubt.id%>"
      class="d-flex my-3 p-2 bg-grey-container border border-2 border-secondary"
    >
      <div class="flex-grow-1"><%=doubt.title %></div>
      <div class>
        <a
          class="btn btn-sm btn-outline-primary"
          href="/ta/open-doubt/?id=<%=doubt.id%>"
          >Accept</a
        >
      </div>
      <div
        class="bg-secondary rounded-pill mx-3"
        style="width: 25px; height: 25px"
      ></div>
    </div>
    <%}%>
  </div>
</div>

